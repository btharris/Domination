<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TextAppearanceMedium" parent="@*android:style/TextAppearance.Medium">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="TextAppearanceSmall" parent="@*android:style/TextAppearance.Small">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#ffdddddd</item>
    </style>
    <style name="ListViewStyle" parent="@*android:style/Widget.Material.ListView">
        <item name="android:overScrollMode">always</item>
    </style>
    <style name="ContactListFilterTheme" parent="@*android:style/Theme.Holo">
        <item name="android:listViewStyle">@style/ListViewStyle</item>
    </style>
    <style name="BackgroundOnlyTheme" parent="@*android:style/Theme.Material">
        <item name="android:windowBackground">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:windowNoDisplay">true</item>
    </style>
    <style name="InCallPopupMenuStyle" parent="@*android:style/Theme.Material">
        <item name="android:textColorPrimary">#ffffffff</item>
    </style>
    <style name="InCallActionBarStyle" parent="@*android:style/Widget.Material.ActionBar">
        <item name="android:textColor">#ffffffff</item>
        <item name="android:background">#ff1976d2</item>
    </style>
    <style name="Theme.InCallScreen" parent="@*android:style/Theme.Material">
        <item name="android:textColorPrimary">#ffffffff</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:windowBackground">@null</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:actionBarStyle">@style/InCallActionBarStyle</item>
        <item name="android:popupMenuStyle">@style/InCallPopupMenuStyle</item>
        <item name="android:alertDialogTheme">@*android:style/Theme.Material.Dialog.Alert</item>
    </style>
    <style name="DialtactsTheme1" parent="@*android:style/Theme.Material">
        <item name="android:colorForeground">@*android:color/background_floating_material_dark</item>
        <item name="android:colorBackground">@*android:color/background_dark</item>
        <item name="android:textColorPrimary">#ffffffff</item>
        <item name="android:textColorSecondary">#ffdddddd</item>
        <item name="android:colorControlActivated">#ff1976d2</item>
        <item name="android:colorPrimary">#ff1976d2</item>
        <item name="android:colorPrimaryDark">#ff0d47a1</item>
    </style>
    <style name="AlertDialogTheme" parent="@*android:style/Theme.Material.Dialog">
        <item name="android:colorBackground">@*android:color/background_floating_material_dark</item>
        <item name="android:colorAccent">#ff1976d2</item>
    </style>
    <style name="CallLogSpinnerStyle">
        <item name="android:textAppearance">@*android:style/TextAppearance.Small</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:singleLine">true</item>
        <item name="android:textAllCaps">true</item>
    </style>


    <style name="DialtactsActionBarOverflow" parent="@*android:style/Widget.Material.ActionButton.Overflow" />
    <style name="DialtactsActionBarOverflowWhite" parent="@*android:style/Widget.Material.ActionButton.Overflow" />
    <style name="DialtactsActionBarTabTextStyle" parent="@*android:style/Widget.Material.ActionBar.TabText" />
</resources>
